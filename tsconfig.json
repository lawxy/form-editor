{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "@/*": [
        "src/form-editor/*"
      ],
      "gfpage": [
        "src/index.ts"
      ],
      "gfpage/*": [
        "./src/*",
        "*"
      ],
      "gfpage/es/*": [
        "src/*"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "src/**/*"
  ]
}